<?xml version="1.0" encoding="UTF-8"?>
<cronentries>
  <cron>
    <url>/worker</url>
    <description>every 120 minutes from 00:00 to 09:50</description>
    <schedule>every 120 minutes from 00:00 to 09:50</schedule>
    <retry-parameters>
      <job-retry-limit>1</job-retry-limit> <!-- retry times -->
      <min-backoff-seconds>3000</min-backoff-seconds> <!-- minimum seconds to wait before retry -->
      <max-backoff-seconds>4200</max-backoff-seconds> <!-- maximum seconds to wait before retry -->
    </retry-parameters>
    <timezone>America/Argentina/Buenos_Aires</timezone>
  </cron>
  <cron>
    <url>/worker</url>
    <description>every 60 minutes from 13:30 to 23:59</description>
    <schedule>every 60 minutes from 13:30 to 23:59</schedule>
    <retry-parameters>
      <job-retry-limit>1</job-retry-limit> <!-- retry times -->
      <min-backoff-seconds>1800</min-backoff-seconds> <!-- minimum seconds to wait before retry -->
      <max-backoff-seconds>2400</max-backoff-seconds> <!-- maximum seconds to wait before retry -->
    </retry-parameters>
    <timezone>America/Argentina/Buenos_Aires</timezone>
  </cron>
  <cron>
    <url>/worker</url>
    <description>every 2 minutes from 09:50 to 10:15</description>
    <schedule>every 2 minutes from 09:50 to 10:15</schedule>
    <timezone>America/Argentina/Buenos_Aires</timezone>
  </cron>
  <cron>
    <url>/worker</url>
    <description>every 10 minutes from 10:15 to 13:15</description>
    <schedule>every 10 minutes from 10:15 to 13:15</schedule>
    <timezone>America/Argentina/Buenos_Aires</timezone>
    <retry-parameters>
      <job-retry-limit>1</job-retry-limit> <!-- retry times -->
      <min-backoff-seconds>240</min-backoff-seconds> <!-- minimum seconds to wait before retry -->
      <max-backoff-seconds>300</max-backoff-seconds> <!-- maximum seconds to wait before retry -->
    </retry-parameters>    
  </cron>
  <cron>
    <url>/ha/keepalive</url>
    <description>keep alive</description>
    <schedule>every 5 minutes</schedule>
  </cron>  
  <cron>
    <url>/ha/cleanup</url>
    <description>expires cleanup</description>
    <schedule>every day 00:00</schedule>
  </cron>
</cronentries>
