<?xml version="1.0" encoding="UTF-8"?>
<cronentries>
  <cron>
    <url>/worker</url>
    <description>every 60 minutes from 00:00 to 08:30</description>
    <schedule>every 60 minutes from 00:00 to 08:30</schedule>
    <retry-parameters>
      <job-retry-limit>1</job-retry-limit> <!-- retry times -->
      <min-backoff-seconds>1800</min-backoff-seconds> <!-- minimum seconds to wait before retry -->
      <max-backoff-seconds>2400</max-backoff-seconds> <!-- maximum seconds to wait before retry -->
    </retry-parameters>
    <timezone>America/Argentina/Buenos_Aires</timezone>
  </cron>
  <cron>
    <url>/worker</url>
    <description>every 60 minutes from 12:30 to 23:59</description>
    <schedule>every 60 minutes from 12:30 to 23:59</schedule>
    <retry-parameters>
      <job-retry-limit>1</job-retry-limit> <!-- retry times -->
      <min-backoff-seconds>1800</min-backoff-seconds> <!-- minimum seconds to wait before retry -->
      <max-backoff-seconds>2400</max-backoff-seconds> <!-- maximum seconds to wait before retry -->
    </retry-parameters>
    <timezone>America/Argentina/Buenos_Aires</timezone>
  </cron>
  <cron>
    <url>/worker</url>
    <description>every 15 minutes from 09:00 to 12:00</description>
    <schedule>every 15 minutes from 09:00 to 12:00</schedule>
    <timezone>America/Argentina/Buenos_Aires</timezone>
  </cron>
  <cron>
    <url>/ha/keepalive</url>
    <description>keep alive</description>
    <schedule>every 5 minutes</schedule>
  </cron>  
</cronentries>
